{
  "name": "marapp-services",
  "version": "1.6.0",
  "description": "Marapp API services",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc --build",
    "watch": "tsc --watch",
    "test": "jest --runInBand --passWithNoTests",
    "docker-compose:test": "docker-compose -f docker-compose.test.yml up --force-recreate",
    "test:integration": "jest --runInBand --passWithNoTests --testPathPattern=tests/integration --detectOpenHandles --forceExit",
    "test:e2e": "jest --runInBand --passWithNoTests --testPathPattern=tests/e2e --detectOpenHandles --forceExit",
    "typecheck": "tsc --noEmit",
    "lint": "tslint --project tslint.tsconfig.json --fix",
    "prettify": "prettier --write '{src,tests,support}/**/*.{js,ts,json}'",
    "serverless:start": "serverless offline --verbose --noPrependStageInUrl",
    "serverless:start:debug": "SLS_DEBUG=1 serverless offline --verbose --noPrependStageInUrl",
    "serverless:package": "serverless package --verbose",
    "serverless:deploy": "serverless deploy --verbose",
    "serverless:deploy:debug": "SLS_DEBUG=1 serverless deploy --verbose",
    "serverless:deploy:local": "SLS_DEBUG=1 serverless deploy --stage local --verbose"
  },
